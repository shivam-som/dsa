// using inbuilt function
int main(){
    int n; cin>>n;
    int a[n];
    for(int i=0; i<n; i++) cin>>a[i];
    reverse(a, a+n);
    for(int i:a) cout<<i<<" ";
    return 0;
}


//Without inbuilt function
int main(){
    int n; cin>>n;
    int a[n];
    for(int i=0; i<n; i++) cin>>a[i];
    int i=0, j=n-1;
    while(i<j){
        a[i] = a[i]+a[j];
        a[j] = a[i]-a[j];
        a[i] = a[i]-a[j];
        i++;
        j--;
    }
    for(int k:a) cout<<k<<" ";
    return 0;
}

