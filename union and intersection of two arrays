// using set method
#include <bits/stdc++.h>
using namespace std;

int main(){
    int a[5]={2,3,4,5,6};
    int b[3]={7,3,4};
    int m=sizeof(a)/sizeof(a[0]);
    int n=sizeof(b)/sizeof(b[0]);
    set<int> s(a, a+m);
    vector<int> intersection;
    for(int i:b) s.insert(i);
    for(int i:s) cout<<i<<" "; //union
    cout<<endl;
    for(int i:a) for(int j:b) if(i==j) intersection.push_back(i);
    for(int i:intersection) cout<<i<<" "; //intersection
    return 0;
}
