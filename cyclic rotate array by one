//make a separate fucntion
#include <bits/stdc++.h>
using namespace std;

void rotateByOne(int arr[], int n){
    int first = arr[0];
    for(int i=0; i<n-1; i++) arr[i]=arr[i+1];
    arr[n-1]=first;
}

int main(){
    int a[5]={2,3,4,5,6};
    rotateByOne(a, 5);
    for(int i:a) cout<<i<<" ";
    return 0;
}
