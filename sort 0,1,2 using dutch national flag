#include <bits/stdc++.h>
using namespace std;

void swap(int *a, int *b){
    int temp = *a;
    *a = *b;
    *b = temp;
}

int * ans(int arr[], int n){
    int low=0;
    int high=n-1;
    for(int i=0; i<=high; i++){
        if(arr[i]==2){
            swap(arr[i--], arr[high--]);
        } else if(arr[i]==0){
            swap(arr[i], arr[low++]);
        }
    }
    return arr;
}

int main(){
    int arr[6]={0, 1, 2, 0, 1, 2};
    int *p;
    p = ans(arr, 6);
    for(int i=0; i<6; i++){
        cout<<*(p+i)<<" ";
    }
    return 0;
}
