#include <bits/stdc++.h>
using namespace std;

void swap(int *a, int *b){
    int temp = *a;
    *a = *b;
    *b = temp;
}

int * ans(int arr[], int n){
    int low=0;
    for(int i=0; i<n; i++){
        if(arr[i]<0){
            swap(arr[i], arr[low++]);
        }
    }
    return arr;
}

int main(){
    int n=8;
    int arr[n]={-4, 1, 2, -5, -1, -2, 6, -1};
    int *p;
    p = ans(arr, n);
    for(int i=0; i<n; i++){
        cout<<*(p+i)<<" ";
    }
    return 0;
}
